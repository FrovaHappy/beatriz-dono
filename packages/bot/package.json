{
  "name": "bot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "generateListServices": "tsx src/generateListServices.ts",
    "start": "tsx src/index.ts",
    "db:push": "npx drizzle-kit push",
    "db:migrate": "npx drizzle-kit generate && npx drizzle-kit migrate"
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": "ts",
    "exec": "rollup -c && tsx index.js"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.67",
    "cors": "2.8.5",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "1.18.1",
    "module-alias": "^2.2.3",
    "passport": "0.7.0",
    "passport-discord": "0.1.4",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/module-alias": "^2.0.4",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.14",
    "ts-node": "10.9.2"
  }
}
