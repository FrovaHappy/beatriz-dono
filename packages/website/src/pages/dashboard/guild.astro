---
import getPublic from "@utils/getPublic";
import Settings from "../../components/Dashboard/Guild/Settings";

import Layout from "../../layouts/Layout.astro";

const pub = getPublic(import.meta.env.PUBLIC_VARIABLES);
---

<Layout title="Guild Management" pub={pub}>
  <h1>Guild Management</h1>
  <Settings
    apiUrl={pub.urlApi}
    oauthCallback={pub.urlOathCallback}
    client:only
  />
</Layout>
